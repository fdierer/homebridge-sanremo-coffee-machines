  "dependencies": {
  
    
  "hap-js": "^1.3.3",
  
    
  "node-fetch": "^2.7.0"
  
  },
  
  "devDependencies": {
  
    
  "@types/node": ">=18.16.20",
  
    
  "@typescript-eslint/eslint-plugin": "^7.1.1",
  
    
  "@typescript-eslint/parser": "^7.1.1",
  
    
  "eslint": ">=8.45.0",
  
    
  "homebridge": "^1.11.1",
  
    
  "nodemon": ">=2.0.22",
  
    
  "rimraf": ">=3.0.2",
  
    
  "ts-node": ">=10.9.1",
  
    
  "typescript": ">=4.9.5"
  
  },
  
  "homepage": "https://github.com/fdierer/homebridge-sanremo-coffee-machines#readme",
