{
  
  "displayName": "Sanremo Cube for Homebridge",
  
  "name": "homebridge-sanremo-cube-coffeemachine",
  
  "version": "1.4.0",
  
  "description": "Homebridge plugin for Sanremo Coffee Machines. Control your Sanremo Cube coffee machine via HomeKit with temperature monitoring, power control, and filter maintenance tracking.",
  
  "license": "MIT",
  
  "module": "commonjs",
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdierer/homebridge-sanremo-coffee-machines.git"
  },
  "bugs": {
    "url": "https://github.com/fdierer/homebridge-sanremo-coffee-machines/issues"
  },
  
  "engines": {
  
    
  "homebridge": "^1.6.0 || ^2.0.0-beta.0",
  
    
  "node": ">=18.20.4 <25"
  
  },
  
  "main": "dist/index.js",
  
  "files": [
    "dist/**/*",
    "config.schema.json",
    "README.md",
    "LICENSE"
  ],
  
  "scripts": {
  
    
  "lint": "eslint src/**.ts --max-warnings=0",
  
    
  "watch": "npm run build && npm link && nodemon",
  
    
  "build": "rimraf ./dist && tsc",
  
    
  "prepublishOnly": "npm run lint && npm run build"
  
  },
  
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "sanremo",
